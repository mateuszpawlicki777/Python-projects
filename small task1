lista2 = ['r','o','o','o','r']
print(lista1)
index = 2
new_value = 'r'
lista2[index] = new_value
print(lista2)
string='Mateusz Pawlicki'
print(string[-3:])
print(lista1.count('o'),'/',len(lista1))
df=pd.DataFrame(zip(lista1,lista2))
print(df)
df.columns=['or≈Çy','reszki']
print(df)
